<% title "Delete #{@resource_suggestion.title}"%>

<%= form_for :resource_suggestion, url: admin_resource_suggestion_path(@resource_suggestion), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @resource_suggestion.title %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_resource_suggestions_path %>
  </p>
<% end %>