<% title "Delete #{@blog_post.title}"%>

<%= form_for @blog_post, url: admin_blog_post_path(@blog_post), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @blog_post.title %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_blog_post_path %>
  </p>
<% end %>