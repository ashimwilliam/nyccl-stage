<% title "Delete #{@system_avatar.name}"%>

<%= form_for :system_avatar, url: admin_system_avatar_path(@system_avatar), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @system_avatar.name %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_system_avatars_path %>
  </p>
<% end %>