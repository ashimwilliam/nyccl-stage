<% title "Delete #{@faq.question}"%>

<%= form_for :faq, url: admin_faq_path(@faq), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @faq.question %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_faqs_path %>
  </p>
<% end %>