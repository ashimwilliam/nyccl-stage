<% title "Delete #{@audience.name}"%>

<%= form_for :audience, url: admin_audience_path(@audience), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @audience.name %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_audiences_path %>
  </p>
<% end %>