<% title "Delete #{@blog_user.email}"%>

<%= form_for @blog_user, url: admin_blog_user_path(@blog_user), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @blog_user.email %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_blog_users_path %>
  </p>
<% end %>