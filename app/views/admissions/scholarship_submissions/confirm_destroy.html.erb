<% title "Delete #{@scholarship_submission.full_name}"%>

<%= form_for :scholarship_submission, url: admin_scholarship_submission_path(@scholarship_submission), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @scholarship_submission.full_name %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_scholarship_submissions_path %>
  </p>
<% end %>