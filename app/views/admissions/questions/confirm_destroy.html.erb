<% title "Delete Question?"%>

<%= form_for :question, url: admin_question_path(@question), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @question.question %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_questions_path %>
  </p>
<% end %>