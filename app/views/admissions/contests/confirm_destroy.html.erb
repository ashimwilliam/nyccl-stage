<% title "Delete #{@contest.name}"%>

<%= form_for :contest, url: admin_contest_path(@contest), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @contest.name %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_contests_path %>
  </p>
<% end %>
