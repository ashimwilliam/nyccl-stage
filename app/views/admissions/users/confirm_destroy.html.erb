<% title "Delete #{@user.full_name}"%>

<%= form_for :user, url: admin_user_path(@user), html: { method: :delete } do |f| %>
  <h3>Are you sure you want to permanently destroy <%= @user.full_name %>?</h3>
  <p>
    <button type="submit">Destroy</button> or <%= link_to "cancel", admin_users_path %>
  </p>
<% end %>